{
  "version": 2,
  "name": "autonautics-waste-management",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/driver/([^/]+)/location",
      "dest": "/api/driver/[driverId]/location.js"
    },
    {
      "src": "/api/driver/([^/]+)/routes",
      "dest": "/api/driver/[driverId]/routes.js"
    },
    {
      "src": "/api/driver/([^/]+)/update",
      "dest": "/api/driver/[driverId]/update.js"
    },
    {
      "src": "/api/driver/([^/]+)/status",
      "dest": "/api/driver/[driverId]/status.js"
    },
    {
      "src": "/api/driver/([^/]+)/fuel",
      "dest": "/api/driver/[driverId]/fuel.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

